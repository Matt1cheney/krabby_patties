<header>

  <div class="container center">

    <h3 class="title">Welcome to the Krusty Krab!</h3>

  </div>

</header>

<div class="container">

  <div class="row">

    <form class="create-form">

      <div class="col s6 m6 l6">

        <input placeholder="What type of Krabby Patty do you want?" type="text" id="burgerReq">

      </div>

      <a class="waves-effect waves-light btn"><i class="burgerBtn fas fa-hamburger material-icons left"></i>add to
        menu!</a>

    </form>
  </div>
  <div class="row">

    <div class="col s12 m5 l5 toBeEaten">
      <ul>
        {{#each burgerReq}}
          {{#unless eaten}}
            {{> cats/eatery eaten=false}}
          {{/unless}}
        {{/each}}
      </ul>
    </div>

    <div class="col s12 m5 l5 eaten right">
      <ul>
  {{#each burgerReq}}
    {{#if eaten}}
      {{> cats/eatery eaten=true}}
    {{/if}}
  {{/each}}
</ul>

    </div>

  </div>

</div>